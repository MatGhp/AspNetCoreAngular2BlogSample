{
  "compilerOptions": {
    
    "target": "es5",
    "module": "commonjs",
    "sourceMap": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "skipDefaultLibCheck": true,
    "lib": [ "es6", "dom" ],
    "types": [ "node" ]
  },
  "exclude": [
    "node_modules",
    "bin",
    "typings/es6-shim/es6-shim.d.ts",
    "typings/tsd.d.ts"
  ],
  "atom": { "rewriteTsconfig": false }
}
